{
    "name": "shylockly",
    "version": "0.0.0",
    "description": "",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "preversion": "rm -rf dist && tsc",
        "postversion": "npm publish",
        "build": "tsc -p tsconfig.build.json",
        "nest": "nest",
        "start": "nest start",
        "start:dev": "run-p start-server:dev start-frontend:dev",
        "start-server:dev": "nest start --watch",
        "start-frontend:dev": "cd frontend && npm run build:dev",
        "start-frontend:prod": "cd frontend && npm install && npm run build:prod",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "prestart:prod": "rimraf dist && npm run build",
        "postinstall:prod": "npm run prestart:prod && npm run start-frontend:prod",
        "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "format": "prettier --config .prettierrc --write \"src/**/*.{ts,tsx}\"",
        "format-check": "prettier --config .prettierrc --check \"src/**/*.{ts,tsx}\""
    },
    "author": "",
    "license": "UNLICENSED",
    "devDependencies": {
        "@nestjs/cli": "^6.12.9",
        "@nestjs/schematics": "^6.7.6",
        "@nestjs/testing": "^6.10.11",
        "@types/bcrypt": "^3.0.0",
        "@types/jest": "^24.0.23",
        "@types/node": "^12.12.6",
        "@types/passport-jwt": "^3.0.3",
        "@types/passport-local": "^1.0.33",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "cron": "^1.8.2",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-autofix": "^1.0.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "pg": "^7.12.1",
        "prettier": "^2.0.5",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.9",
        "@nestjs/core": "^7.0.9",
        "@nestjs/jwt": "^7.0.0",
        "@nestjs/passport": "^7.0.0",
        "@nestjs/platform-express": "^7.0.9",
        "@nestjs/schedule": "^0.3.1",
        "@nestjs/serve-static": "^2.1.0",
        "@nestjs/swagger": "^4.5.1",
        "@nestjs/typeorm": "^7.0.0",
        "@types/cron": "^1.7.2",
        "@types/pluralize": "0.0.29",
        "@types/request-promise-native": "^1.0.17",
        "@types/sinon": "^7.5.1",
        "axios-jwt": "^1.2.0",
        "bcrypt": "^4.0.1",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "cron": "^1.8.2",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.0",
